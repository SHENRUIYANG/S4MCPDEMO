<?xml version="1.0" encoding="utf-8"?>
<edmx:Edmx Version="4.0" xmlns:edmx="http://docs.oasis-open.org/odata/ns/edmx">
  <edmx:Reference Uri="https://oasis-tcs.github.io/odata-vocabularies/vocabularies/Org.OData.Authorization.V1.xml">
    <edmx:Include Alias="Authorization" Namespace="Org.OData.Authorization.V1"/>
  </edmx:Reference>
  <edmx:Reference Uri="https://oasis-tcs.github.io/odata-vocabularies/vocabularies/Org.OData.Capabilities.V1.xml">
    <edmx:Include Alias="Capabilities" Namespace="Org.OData.Capabilities.V1"/>
  </edmx:Reference>
  <edmx:Reference Uri="https://sap.github.io/odata-vocabularies/vocabularies/Common.xml">
    <edmx:Include Alias="Common" Namespace="com.sap.vocabularies.Common.v1"/>
  </edmx:Reference>
  <edmx:Reference Uri="https://oasis-tcs.github.io/odata-vocabularies/vocabularies/Org.OData.Core.V1.xml">
    <edmx:Include Alias="Core" Namespace="Org.OData.Core.V1"/>
  </edmx:Reference>
  <edmx:Reference Uri="https://sap.github.io/odata-vocabularies/vocabularies/UI.xml">
    <edmx:Include Alias="UI" Namespace="com.sap.vocabularies.UI.v1"/>
  </edmx:Reference>
  <edmx:Reference Uri="https://oasis-tcs.github.io/odata-vocabularies/vocabularies/Org.OData.Validation.V1.xml">
    <edmx:Include Alias="Validation" Namespace="Org.OData.Validation.V1"/>
  </edmx:Reference>
  <edmx:DataServices>
    <Schema Namespace="sap.billing.sb.analytics.ExchangeRateService" xmlns="http://docs.oasis-open.org/odata/ns/edm">
      <EntityContainer Name="EntityContainer">
        <EntitySet Name="ExchangeRates" EntityType="sap.billing.sb.analytics.ExchangeRateService.ExchangeRates"/>
        <EntitySet Name="ExchangeRateMdiReplicationReports" EntityType="sap.billing.sb.analytics.ExchangeRateService.ExchangeRateMdiReplicationReports"/>
      </EntityContainer>
      <EntityType Name="ExchangeRates">
        <Key>
          <PropertyRef Name="ExchangeRateOID"/>
        </Key>
        <Property Name="ExchangeRateOID" Type="Edm.String" MaxLength="128" Nullable="false"/>
        <Property Name="ExchangeRateDisplayID" Type="Edm.String" MaxLength="21" Nullable="false"/>
        <Property Name="SourceCurrencyISOCode" Type="Edm.String" MaxLength="3" Nullable="false"/>
        <Property Name="TargetCurrencyISOCode" Type="Edm.String" MaxLength="3" Nullable="false"/>
        <Property Name="ExchangeRateEffectiveDate" Type="Edm.Date" Nullable="false"/>
        <Property Name="ExchangeRate" Type="Edm.Decimal" Precision="9" Scale="5"/>
        <Property Name="IsDirectExchangeRate" Type="Edm.Boolean" Nullable="false"/>
        <Property Name="NumberOfSourceCurrencyUnits" Type="Edm.Decimal" Precision="9" Scale="0"/>
        <Property Name="NumberOfTargetCurrencyUnits" Type="Edm.Decimal" Precision="9" Scale="0"/>
        <Property Name="LastChangeDateTime" Type="Edm.DateTimeOffset" Precision="7" Nullable="false"/>
      </EntityType>
      <EntityType Name="ExchangeRateMdiReplicationReports">
        <Key>
          <PropertyRef Name="MDIntegBusObjectInstanceID"/>
        </Key>
        <Property Name="MDIntegBusObjectInstanceID" Type="Edm.String" MaxLength="128" Nullable="false"/>
        <Property Name="ExchangeRateOID" Type="Edm.String" MaxLength="128"/>
        <Property Name="SourceCurrencyISOCode" Type="Edm.String" MaxLength="3"/>
        <Property Name="TargetCurrencyISOCode" Type="Edm.String" MaxLength="3"/>
        <Property Name="ExchangeRateType" Type="Edm.String" MaxLength="4"/>
        <Property Name="ExchangeRateEffectiveDate" Type="Edm.Date"/>
        <Property Name="ExchangeRateReplicationStatus" Type="Edm.String"/>
        <Property Name="ExchangeRateReplicationErrorText" Type="Edm.String"/>
        <Property Name="LastChangeDateTime" Type="Edm.DateTimeOffset" Precision="7"/>
      </EntityType>
      <Annotation Term="Core.SchemaVersion" String="1"/>
      <Annotations Target="sap.billing.sb.analytics.ExchangeRateService.EntityContainer">
        <Annotation Term="Capabilities.BatchSupported" Bool="false"/>
        <Annotation Term="Common.Label" String="Exchange Rates"/>
        <Annotation Term="Core.Description" String="Manage exchange rates for analytics."/>
        <Annotation Term="Core.LongDescription" String="Enables you to create and manage exchange rate data for analytics. You can create, change and search for exchange rates. You can also retrieve the status of replicating exchange rates from SAP Master Data Integration."/>
        <Annotation Term="Authorization.SecuritySchemes">
          <Collection>
            <Record Type="Authorization.SecurityScheme">
              <PropertyValue Property="Authorization" String="oauth2"/>
              <PropertyValue Property="RequiredScopes">
                <Collection>
                  <String>{identityzone}.analytics-exchange-rate_view</String>
                  <String>{identityzone}.analytics-exchange-rate_manage</String>
                  <String>{identityzone}.mdi-replication-report_view</String>
                </Collection>
              </PropertyValue>
            </Record>
          </Collection>
        </Annotation>
        <Annotation Term="Authorization.Authorizations">
          <Collection>
            <Record Type="Authorization.OAuth2ClientCredentials">
              <PropertyValue Property="Description" String="To use this API, log on to the SAP BTP (Business Technology Platform) cockpit to get a service key with OAuth client credentials. Then use the service key to generate an access/bearer token. &lt;br>For more information, see the API Guide."/>
              <PropertyValue Property="Name" String="oauth2"/>
              <PropertyValue Property="TokenUrl" String="https://{identityzone}.authentication.{region}.hana.ondemand.com/oauth/token"/>
              <PropertyValue Property="Scopes">
                <Collection>
                  <Record Type="Authorization.AuthorizationScope">
                    <PropertyValue Property="Scope" String="{identityzone}.analytics-exchange-rate_view"/>
                    <PropertyValue Property="Description" String="View exchange rates."/>
                  </Record>
                  <Record Type="Authorization.AuthorizationScope">
                    <PropertyValue Property="Scope" String="{identityzone}.analytics-exchange-rate_manage"/>
                    <PropertyValue Property="Description" String="Manage exchange rates."/>
                  </Record>
                  <Record Type="Authorization.AuthorizationScope">
                    <PropertyValue Property="Scope" String="{identityzone}.mdi-replication-report_view"/>
                    <PropertyValue Property="Description" String="View SAP Master Data Integration replication reports."/>
                  </Record>
                </Collection>
              </PropertyValue>
            </Record>
          </Collection>
        </Annotation>
      </Annotations>
      <Annotations Target="sap.billing.sb.analytics.ExchangeRateService.ExchangeRates">
        <Annotation Term="Common.Label" String="Exchange Rates Management"/>
        <Annotation Term="Core.Description" String="Exchange Rates"/>
      </Annotations>
      <Annotations Target="sap.billing.sb.analytics.ExchangeRateService.EntityContainer/ExchangeRates">
        <Annotation Term="Capabilities.ReadRestrictions">
          <Record Type="Capabilities.ReadRestrictionsType">
            <PropertyValue Property="Description" String="Retrieves exchange rates."/>
            <PropertyValue Property="LongDescription" String="Retrieves a collection of exchange rates that match the search criteria.&lt;br>&lt;br>This operation requires the scope `analytics-exchange-rate_view`."/>
            <PropertyValue Property="ReadByKeyRestrictions">
              <Record Type="Capabilities.ReadByKeyRestrictionsType">
                <PropertyValue Property="Description" String="Retrieves an exchange rate."/>
                <PropertyValue Property="LongDescription" String="Retrieves a single exchange rate with a specific identifier.&lt;br>&lt;br>This operation requires the scope `analytics-exchange-rate_view`."/>
              </Record>
            </PropertyValue>
          </Record>
        </Annotation>
        <Annotation Term="Capabilities.InsertRestrictions">
          <Record Type="Capabilities.InsertRestrictionsType">
            <PropertyValue Property="Description" String="Creates an exchange rate."/>
            <PropertyValue Property="LongDescription" String="Creates a new exchange rate.&lt;br>&lt;br>This operation requires the scope `analytics-exchange-rate_manage`."/>
          </Record>
        </Annotation>
        <Annotation Term="Capabilities.UpdateRestrictions">
          <Record Type="Capabilities.UpdateRestrictionsType">
            <PropertyValue Property="UpdateMethod" EnumMember="Capabilities.HttpMethod/PATCH"/>
            <PropertyValue Property="Description" String="Patches an exchange rate."/>
            <PropertyValue Property="LongDescription" String="Patches a single exchange rate with a specific identifier.&lt;br>&lt;br>This operation requires the scope `analytics-exchange-rate_manage`."/>
          </Record>
        </Annotation>
        <Annotation Term="Capabilities.DeleteRestrictions">
          <Record Type="Capabilities.DeleteRestrictionsType">
            <PropertyValue Property="Description" String="Deletes an exchange rate."/>
            <PropertyValue Property="LongDescription" String="Deletes a single exchange rate with a specific identifier.&lt;br>&lt;br>This operation requires the scope `analytics-exchange-rate_manage`."/>
          </Record>
        </Annotation>
      </Annotations>
      <Annotations Target="sap.billing.sb.analytics.ExchangeRateService.ExchangeRates/ExchangeRateOID">
        <Annotation Term="Core.Description" String="Exchange Rate ID"/>
        <Annotation Term="Core.LongDescription" String="The unique identifier of the exchange rate."/>
        <Annotation Term="Core.Example">
          <Record Type="Core.PrimitiveExampleValue">
            <PropertyValue Property="Value" String="TS1FVVIvVVNELTIwMjQwMTAx"/>
          </Record>
        </Annotation>
        <Annotation Term="Core.Computed" Bool="true"/>
      </Annotations>
      <Annotations Target="sap.billing.sb.analytics.ExchangeRateService.ExchangeRates/ExchangeRateDisplayID">
        <Annotation Term="Core.LongDescription" String="The human-readable identifier of the exchange rate."/>
        <Annotation Term="Core.Example">
          <Record Type="Core.PrimitiveExampleValue">
            <PropertyValue Property="Value" String="M-EUR/USD-20240101"/>
          </Record>
        </Annotation>
        <Annotation Term="Core.Computed" Bool="true"/>
      </Annotations>
      <Annotations Target="sap.billing.sb.analytics.ExchangeRateService.ExchangeRates/SourceCurrencyISOCode">
        <Annotation Term="Core.Immutable" Bool="true"/>
        <Annotation Term="Core.LongDescription" String="The source currency code of the exchange rate."/>
        <Annotation Term="Core.Example">
          <Record Type="Core.PrimitiveExampleValue">
            <PropertyValue Property="Value" String="EUR"/>
          </Record>
        </Annotation>
        <Annotation Term="Common.FieldControl" EnumMember="Common.FieldControlType/Mandatory"/>
      </Annotations>
      <Annotations Target="sap.billing.sb.analytics.ExchangeRateService.ExchangeRates/TargetCurrencyISOCode">
        <Annotation Term="Core.Immutable" Bool="true"/>
        <Annotation Term="Core.LongDescription" String="The target currency code of the exchange rate."/>
        <Annotation Term="Core.Example">
          <Record Type="Core.PrimitiveExampleValue">
            <PropertyValue Property="Value" String="USD"/>
          </Record>
        </Annotation>
        <Annotation Term="Common.FieldControl" EnumMember="Common.FieldControlType/Mandatory"/>
      </Annotations>
      <Annotations Target="sap.billing.sb.analytics.ExchangeRateService.ExchangeRates/ExchangeRateEffectiveDate">
        <Annotation Term="Core.Immutable" Bool="true"/>
        <Annotation Term="Core.LongDescription" String="The effective date of the exchange rate. The date must be later than 1900-01-01."/>
        <Annotation Term="Core.Example">
          <Record Type="Core.PrimitiveExampleValue">
            <PropertyValue Property="Value" String="2024-01-01"/>
          </Record>
        </Annotation>
        <Annotation Term="Common.FieldControl" EnumMember="Common.FieldControlType/Mandatory"/>
        <Annotation Term="Validation.Minimum" String="1900-01-01"/>
        <Annotation Term="Validation.Maximum" String="9999-12-31"/>
      </Annotations>
      <Annotations Target="sap.billing.sb.analytics.ExchangeRateService.ExchangeRates/ExchangeRate">
        <Annotation Term="Core.LongDescription" String="Exchange rate value."/>
        <Annotation Term="Core.Example">
          <Record Type="Core.PrimitiveExampleValue">
            <PropertyValue Property="Value" Float="1.1"/>
          </Record>
        </Annotation>
        <Annotation Term="Common.FieldControl" EnumMember="Common.FieldControlType/Mandatory"/>
        <Annotation Term="Validation.Minimum" Float="0.00001"/>
        <Annotation Term="Validation.Maximum" Float="9999.99999"/>
      </Annotations>
      <Annotations Target="sap.billing.sb.analytics.ExchangeRateService.ExchangeRates/IsDirectExchangeRate">
        <Annotation Term="Core.LongDescription" String="Indicates whether the rate is a direct quotation. If set to true, the rate represents the quantity of target currency needed to purchase one unit of source currency."/>
        <Annotation Term="Core.Example">
          <Record Type="Core.PrimitiveExampleValue">
            <PropertyValue Property="Value" Bool="true"/>
          </Record>
        </Annotation>
        <Annotation Term="Common.FieldControl" EnumMember="Common.FieldControlType/Mandatory"/>
      </Annotations>
      <Annotations Target="sap.billing.sb.analytics.ExchangeRateService.ExchangeRates/NumberOfSourceCurrencyUnits">
        <Annotation Term="Core.LongDescription" String="The ratio factor for the source currency units. The value must be a power of 10."/>
        <Annotation Term="Core.Example">
          <Record Type="Core.PrimitiveExampleValue">
            <PropertyValue Property="Value" Int="1"/>
          </Record>
        </Annotation>
        <Annotation Term="Common.FieldControl" EnumMember="Common.FieldControlType/Mandatory"/>
        <Annotation Term="Validation.Minimum" Int="1"/>
        <Annotation Term="Validation.Maximum" Int="100000000"/>
      </Annotations>
      <Annotations Target="sap.billing.sb.analytics.ExchangeRateService.ExchangeRates/NumberOfTargetCurrencyUnits">
        <Annotation Term="Core.LongDescription" String="The ratio factor for the target currency units. The value must be a power of 10"/>
        <Annotation Term="Core.Example">
          <Record Type="Core.PrimitiveExampleValue">
            <PropertyValue Property="Value" Int="1"/>
          </Record>
        </Annotation>
        <Annotation Term="Common.FieldControl" EnumMember="Common.FieldControlType/Mandatory"/>
        <Annotation Term="Validation.Minimum" Int="1"/>
        <Annotation Term="Validation.Maximum" Int="100000000"/>
      </Annotations>
      <Annotations Target="sap.billing.sb.analytics.ExchangeRateService.ExchangeRates/LastChangeDateTime">
        <Annotation Term="Core.LongDescription" String="Time stamp when the exchange rate is last changed."/>
        <Annotation Term="Core.Example">
          <Record Type="Core.PrimitiveExampleValue">
            <PropertyValue Property="Value" String="2024-04-29T07:06:45.980228600Z"/>
          </Record>
        </Annotation>
        <Annotation Term="Core.Computed" Bool="true"/>
      </Annotations>
      <Annotations Target="sap.billing.sb.analytics.ExchangeRateService.ExchangeRateMdiReplicationReports">
        <Annotation Term="Common.Label" String="Replication Monitoring"/>
        <Annotation Term="Core.Description" String="Exchange Rate Replication Reports"/>
      </Annotations>
      <Annotations Target="sap.billing.sb.analytics.ExchangeRateService.EntityContainer/ExchangeRateMdiReplicationReports">
        <Annotation Term="Capabilities.ReadRestrictions">
          <Record Type="Capabilities.ReadRestrictionsType">
            <PropertyValue Property="Description" String="Retrieves SAP Master Data Integration exchange rate replication reports."/>
            <PropertyValue Property="LongDescription" String="Retrieves a collection of SAP Master Data Integration replication reports that match the search criteria.&lt;br>&lt;br>This operation requires the scope `mdi-replication-report_view`."/>
            <PropertyValue Property="ReadByKeyRestrictions">
              <Record Type="Capabilities.ReadByKeyRestrictionsType">
                <PropertyValue Property="Description" String="Retrieves an SAP Master Data Integration exchange rate replication report."/>
                <PropertyValue Property="LongDescription" String="Retrieves a single SAP Master Data Integration replication report with a specific identifier.&lt;br>&lt;br>This operation requires the scope `mdi-replication-report_view`."/>
              </Record>
            </PropertyValue>
          </Record>
        </Annotation>
        <Annotation Term="Capabilities.DeleteRestrictions">
          <Record Type="Capabilities.DeleteRestrictionsType">
            <PropertyValue Property="Deletable" Bool="false"/>
          </Record>
        </Annotation>
        <Annotation Term="Capabilities.InsertRestrictions">
          <Record Type="Capabilities.InsertRestrictionsType">
            <PropertyValue Property="Insertable" Bool="false"/>
          </Record>
        </Annotation>
        <Annotation Term="Capabilities.UpdateRestrictions">
          <Record Type="Capabilities.UpdateRestrictionsType">
            <PropertyValue Property="Updatable" Bool="false"/>
          </Record>
        </Annotation>
      </Annotations>
      <Annotations Target="sap.billing.sb.analytics.ExchangeRateService.ExchangeRateMdiReplicationReports/MDIntegBusObjectInstanceID">
        <Annotation Term="Core.Description" String="MDI Exchange Rate ID"/>
        <Annotation Term="Core.LongDescription" String="The unique identifier of the exchange rate in SAP Master Data Integration. It is also the identifier of the replication report."/>
        <Annotation Term="Core.Example">
          <Record Type="Core.PrimitiveExampleValue">
            <PropertyValue Property="Value" String="TS1FVVIvVVNELTIwMjQwMTAx"/>
          </Record>
        </Annotation>
      </Annotations>
      <Annotations Target="sap.billing.sb.analytics.ExchangeRateService.ExchangeRateMdiReplicationReports/ExchangeRateOID">
        <Annotation Term="Core.Description" String="Exchange Rate ID"/>
        <Annotation Term="Core.LongDescription" String="The unique identifier of the exchange rate in SAP Subscription Billing."/>
        <Annotation Term="Core.Example">
          <Record Type="Core.PrimitiveExampleValue">
            <PropertyValue Property="Value" String="TS1FVVIvVVNELTIwMjQwMTAx"/>
          </Record>
        </Annotation>
      </Annotations>
      <Annotations Target="sap.billing.sb.analytics.ExchangeRateService.ExchangeRateMdiReplicationReports/SourceCurrencyISOCode">
        <Annotation Term="Core.LongDescription" String="The source currency code of the exchange rate."/>
        <Annotation Term="Core.Example">
          <Record Type="Core.PrimitiveExampleValue">
            <PropertyValue Property="Value" String="EUR"/>
          </Record>
        </Annotation>
      </Annotations>
      <Annotations Target="sap.billing.sb.analytics.ExchangeRateService.ExchangeRateMdiReplicationReports/TargetCurrencyISOCode">
        <Annotation Term="Core.LongDescription" String="The target currency code of the exchange rate."/>
        <Annotation Term="Core.Example">
          <Record Type="Core.PrimitiveExampleValue">
            <PropertyValue Property="Value" String="USD"/>
          </Record>
        </Annotation>
      </Annotations>
      <Annotations Target="sap.billing.sb.analytics.ExchangeRateService.ExchangeRateMdiReplicationReports/ExchangeRateType">
        <Annotation Term="Core.LongDescription" String="Exchange rate type. Currently, only `M` is supported."/>
        <Annotation Term="Core.Example">
          <Record Type="Core.PrimitiveExampleValue">
            <PropertyValue Property="Value" String="M"/>
          </Record>
        </Annotation>
      </Annotations>
      <Annotations Target="sap.billing.sb.analytics.ExchangeRateService.ExchangeRateMdiReplicationReports/ExchangeRateEffectiveDate">
        <Annotation Term="Core.LongDescription" String="The effective date of the exchange rate."/>
        <Annotation Term="Core.Example">
          <Record Type="Core.PrimitiveExampleValue">
            <PropertyValue Property="Value" String="2024-01-01"/>
          </Record>
        </Annotation>
      </Annotations>
      <Annotations Target="sap.billing.sb.analytics.ExchangeRateService.ExchangeRateMdiReplicationReports/ExchangeRateReplicationStatus">
        <Annotation Term="Core.LongDescription" String="The result status of the exchange rate replication."/>
        <Annotation Term="Core.Example">
          <Record Type="Core.PrimitiveExampleValue">
            <PropertyValue Property="Value" String="FAILURE"/>
          </Record>
        </Annotation>
        <Annotation Term="Validation.AllowedValues">
          <Collection>
            <Record Type="Validation.AllowedValue">
              <Annotation Term="Core.SymbolicName" String="SUCCESS"/>
              <PropertyValue Property="Value" String="SUCCESS"/>
            </Record>
            <Record Type="Validation.AllowedValue">
              <Annotation Term="Core.SymbolicName" String="FAILURE"/>
              <PropertyValue Property="Value" String="FAILURE"/>
            </Record>
            <Record Type="Validation.AllowedValue">
              <Annotation Term="Core.SymbolicName" String="SKIPPED"/>
              <PropertyValue Property="Value" String="SKIPPED"/>
            </Record>
          </Collection>
        </Annotation>
      </Annotations>
      <Annotations Target="sap.billing.sb.analytics.ExchangeRateService.ExchangeRateMdiReplicationReports/ExchangeRateReplicationErrorText">
        <Annotation Term="Core.LongDescription" String="The error message of the exchange rate replication."/>
        <Annotation Term="Core.Example">
          <Record Type="Core.PrimitiveExampleValue">
            <PropertyValue Property="Value" String="Currency code not found."/>
          </Record>
        </Annotation>
      </Annotations>
      <Annotations Target="sap.billing.sb.analytics.ExchangeRateService.ExchangeRateMdiReplicationReports/LastChangeDateTime">
        <Annotation Term="Core.LongDescription" String="Time stamp when the replication report is last changed."/>
        <Annotation Term="Core.Example">
          <Record Type="Core.PrimitiveExampleValue">
            <PropertyValue Property="Value" String="2024-04-29T07:06:45.980228600Z"/>
          </Record>
        </Annotation>
        <Annotation Term="Core.Computed" Bool="true"/>
        <Annotation Term="UI.HiddenFilter" Bool="true"/>
        <Annotation Term="Common.Label" String="{i18n>ChangedAt}"/>
      </Annotations>
    </Schema>
  </edmx:DataServices>
</edmx:Edmx>