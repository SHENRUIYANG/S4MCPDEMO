          "MDIntegBusObjectInstanceID": {
            "type": "string",
            "maxLength": 128,
            "example": "TS1FVVIvVVNELTIwMjQwMTAx",
            "description": "The unique identifier of the exchange rate in SAP Master Data Integration. It is also the identifier of the replication report."
          },
          "ExchangeRateOID": {
            "type": "string",
            "maxLength": 128,
            "nullable": true,
            "example": "TS1FVVIvVVNELTIwMjQwMTAx",
            "description": "The unique identifier of the exchange rate in SAP Subscription Billing."
          },
          "SourceCurrencyISOCode": {
            "type": "string",
            "maxLength": 3,
            "nullable": true,
            "example": "EUR",
            "description": "The source currency code of the exchange rate."
          },
          "TargetCurrencyISOCode": {
            "type": "string",
            "maxLength": 3,
            "nullable": true,
            "example": "USD",
            "description": "The target currency code of the exchange rate."
          },
          "ExchangeRateType": {
            "type": "string",
            "maxLength": 4,
            "nullable": true,
            "example": "M",
            "description": "Exchange rate type. Currently, only `M` is supported."
          },
          "ExchangeRateEffectiveDate": {
            "type": "string",
            "format": "date",
            "example": "2024-01-01",
            "nullable": true,
            "description": "The effective date of the exchange rate."
          },
          "ExchangeRateReplicationStatus": {
            "type": "string",
            "enum": [
              "SUCCESS",
              "FAILURE",
              "SKIPPED"
            ],
            "nullable": true,
            "example": "FAILURE",
            "description": "The result status of the exchange rate replication."
          },
          "ExchangeRateReplicationErrorText": {
            "type": "string",
            "nullable": true,
            "example": "Currency code not found.",
            "description": "The error message of the exchange rate replication."
          },
          "LastChangeDateTime": {
            "type": "string",
            "format": "date-time",
            "example": "2024-04-29T07:06:45.980228600Z",
            "nullable": true,
            "description": "Time stamp when the replication report is last changed."
          }
        }
      },
      "sap.billing.sb.analytics.ExchangeRateService.ExchangeRates": {
        "title": "Exchange Rates",
        "type": "object",
        "properties": {
          "ExchangeRateOID": {
            "type": "string",
            "maxLength": 128,
            "example": "TS1FVVIvVVNELTIwMjQwMTAx",
            "description": "The unique identifier of the exchange rate."
          },
          "ExchangeRateDisplayID": {
            "type": "string",
            "maxLength": 21,
            "example": "M-EUR/USD-20240101",
            "description": "The human-readable identifier of the exchange rate."
          },
          "SourceCurrencyISOCode": {
            "type": "string",
            "maxLength": 3,
            "example": "EUR",
            "description": "The source currency code of the exchange rate."
          },
          "TargetCurrencyISOCode": {
            "type": "string",
            "maxLength": 3,
            "example": "USD",
            "description": "The target currency code of the exchange rate."
          },
          "ExchangeRateEffectiveDate": {
            "type": "string",
            "format": "date",
            "example": "2024-01-01",
            "description": "The effective date of the exchange rate. The date must be later than 1900-01-01."
          },
          "ExchangeRate": {
            "anyOf": [
              {
                "type": "number",
                "format": "decimal",
                "multipleOf": 0.00001,
                "maximum": 9999.99999,
                "minimum": 0.00001
              },
              {
                "type": "string"
              }
            ],
            "example": 1.1,
            "x-sap-precision": 9,
            "x-sap-scale": 5,
            "nullable": true,
            "description": "Exchange rate value."
          },
          "IsDirectExchangeRate": {
            "type": "boolean",
            "example": true,
            "description": "Indicates whether the rate is a direct quotation. If set to true, the rate represents the quantity of target currency needed to purchase one unit of source currency."
          },
          "NumberOfSourceCurrencyUnits": {
            "anyOf": [
              {
                "type": "number",
                "format": "decimal",
                "multipleOf": 1,
                "maximum": 100000000,
                "minimum": 1
              },
              {
                "type": "string"
              }
            ],
            "example": 1,
            "x-sap-precision": 9,
            "x-sap-scale": 0,
            "nullable": true,
            "description": "The ratio factor for the source currency units. The value must be a power of 10."
          },
          "NumberOfTargetCurrencyUnits": {
            "anyOf": [
              {
                "type": "number",
                "format": "decimal",
                "multipleOf": 1,
                "maximum": 100000000,
                "minimum": 1
              },
              {
                "type": "string"
              }
            ],
            "example": 1,
            "x-sap-precision": 9,
            "x-sap-scale": 0,
            "nullable": true,
            "description": "The ratio factor for the target currency units. The value must be a power of 10"
          },
          "LastChangeDateTime": {
            "type": "string",
            "format": "date-time",
            "example": "2024-04-29T07:06:45.980228600Z",
            "description": "Time stamp when the exchange rate is last changed."
          }
        }
      },
