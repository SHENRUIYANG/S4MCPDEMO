{
  "openapi": "3.0.2",
  "info": {
    "title": "Exchange Rates",
    "description": "Enables you to create and manage exchange rate data for analytics. You can create, change and search for exchange rates. You can also retrieve the status of replicating exchange rates from SAP Master Data Integration.",
    "version": "1"
  },
  "x-sap-api-type": "ODATAV4",
  "x-odata-version": "4.01",
  "x-sap-shortText": "Manage exchange rates for analytics.",
  "servers": [
    {
      "url": "https://sandbox.api.sap.com/subscriptionbilling/api/analytics/v1"
    },
    {
      "url": "https://{region}.revenue.cloud.sap/api/analytics/v1",
      "description": "SAP Subscription Billing API Endpoint",
      "variables": {
        "region": {
          "description": "SAP Subscription Billing Region",
          "enum": [
            "eu10",
            "us10"
          ],
          "default": "eu10"
        }
      }
    }
  ],
  "tags": [
    {
      "name": "Exchange Rates Management"
    },
    {
      "name": "Replication Monitoring"
    }
  ],
